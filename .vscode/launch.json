{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Jekyll Server",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "bash",
        "args": ["-c", "bundle exec jekyll serve --host 0.0.0.0 --port 4000"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal"
      }
    ]
  }